{
  "name": "n8n-image-generator-mcp",
  "version": "1.0.0",
  "description": "MCP Server for n8n image generation with SSE support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "n8n-image-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:sse": "node dist/sse-server.js",
    "dev": "tsx src/index.ts",
    "dev:sse": "tsx src/sse-server.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "n8n",
    "image-generation",
    "ai",
    "sse",
    "cloudflare",
    "replicate"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 